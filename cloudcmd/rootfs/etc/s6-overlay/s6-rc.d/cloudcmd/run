#!/command/with-contenv bashio
# shellcheck shell=bash
# ==============================================================================
# barisozdag's Personal Home Assistant Add-ons: CloudCMD
# Runs the CloudCMD
# ==============================================================================
bashio::log.info 'Starting the CloudCMD...'

export CLOUDCMD_TERMINAL=false
export CLOUDCMD_OPEN=false

# Run the CloudCMD
exec cloudcmd
